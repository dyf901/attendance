<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jjjt.attendance.dao.StaffLogDao">
    <!--增加员工日志对应关系-->
    <insert id="InsertStaffLog" parameterType="java.util.Map">
        INSERT INTO `attendance`.`staff_log` (`staff_id`, `log_id`) VALUES (#{staff_id},#{log_id});
    </insert>

    <!--根据staff_id查询对应关系-->
    <select id="FindStaffLog" parameterType="java.util.Map" resultType="int">
        SELECT `log_id` FROM `attendance`.`staff_log` WHERE `staff_id`=#{staff_id}
    </select>
</mapper>