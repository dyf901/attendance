<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jjjt.attendance.dao.StaffExamineDao">
    <!--增加员工审批关联表-->
    <insert id="InsertStaffExamine" parameterType="java.util.Map">
        INSERT INTO `attendance`.`staff_examine` (`staff_id`, `examine_id`) VALUES (#{staff_id},#{examine_id});
    </insert>

    <!--根据staff_id查询审批信息-->
    <select id="FindExamineId" parameterType="java.util.Map" resultType="int">
        SELECT `examine_id` FROM `attendance`.`staff_examine` WHERE `staff_id`=#{staff_id}
    </select>
</mapper>